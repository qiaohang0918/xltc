package com.qigan.qiganshop.pojo.game;

import java.util.Date;

/**
 *  使用道具记录(下蛋是一个特殊的道具操作)
 * @author starlord
 */
public class RedeemRecord {
    private RedeemType redeemType;
    private Date redeemDate;
    private String userId;
    private int points;
    private Date createTime;

    private RedeemRecord() {
    }

    public RedeemRecord(RedeemType redeemType, String userId, int points) {
        this.redeemType = redeemType;
        this.userId = userId;
        this.points = points;
    }

    public RedeemType getRedeemType() {
        return redeemType;
    }

    public Date getRedeemDate() {
        return redeemDate;
    }

    public String getUserId() {
        return userId;
    }

    public int getPoints() {
        return points;
    }

    public Date getCreateTime() {
        return createTime;
    }
}
