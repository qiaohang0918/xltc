package com.qigan.qiganshop.pojo.synchronization;

import io.swagger.annotations.ApiModelProperty;

/**
 * 商品
 *
 * @author wanghao
 * @time 2019-05-04 01:44
 */
public class GuanYiItem {
    @ApiModelProperty(value = "商品代码")
    private String item_code;
    @ApiModelProperty(value = "规格代码")
    private String sku_code;
    @ApiModelProperty(value = "标准单价")
    private String origin_price;
    @ApiModelProperty(value = "实际单价")
    private String price;
    @ApiModelProperty(value = "商品数量")
    private int qty;

    @ApiModelProperty(value = "退款状态0:未退款1:退款完成2:退款中")
    private int refund;
    @ApiModelProperty(value = "子订单ID")
    private String oid;

    public String getItem_code() {
        return item_code;
    }

    public void setItem_code(String item_code) {
        this.item_code = item_code;
    }

    public String getSku_code() {
        return sku_code;
    }

    public void setSku_code(String sku_code) {
        this.sku_code = sku_code;
    }

    public String getOrigin_price() {
        return origin_price;
    }

    public void setOrigin_price(String origin_price) {
        this.origin_price = origin_price;
    }

    public String getPrice() {
        return price;
    }

    public void setPrice(String price) {
        this.price = price;
    }

    public int getRefund() {
        return refund;
    }

    public void setRefund(int refund) {
        this.refund = refund;
    }

    public String getOid() {
        return oid;
    }

    public void setOid(String oid) {
        this.oid = oid;
    }

    public int getQty() {
        return qty;
    }

    public void setQty(int qty) {
        this.qty = qty;
    }
}
